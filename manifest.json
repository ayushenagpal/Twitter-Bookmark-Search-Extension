{
    "manifest_version": 3,
    "name": "Twitter Bookmark Search",
    "version": "1.0",
    "permissions": [
      "storage",
      "tabs",
      "https://api.twitter.com/"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    },
    "icons": {
      "16": "icon-16.png",
      "48": "icon-48.png",
      "128": "icon-128.png"
    }
  }
  